{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "airbnb",
      "prettier"
  ],
  "settings": {
    "import/resolver": {
      "node": {
        "moduleDirectory": ["node_modules", "src/"],
        "extensions": [".js", ".jsx"]
      }
    }
  },
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "plugins": [
      "react", "jsx-a11y", "eslint-plugin-prettier", "prettier", "react-hooks"
  ],
  "rules": {
  "react/prefer-stateless-function": 0,
  "react/button-has-type": "off",
  "space-before-function-paren": [
    "error",
    "never"
  ],
  "import/no-named-as-default": 0,
  "import/no-cycle": "off",
  "prettier/prettier": [
    "warn",
    {
      "singleQuote": true,
      "jsxSingleQuote": true,
      "tabWidth": 2,
      "semi": true,
      "arrowParens": "always",
      "bracketSpacing": true
    }
  ],
  "react/jsx-fragments": ["warn", "element"],
  "import/named": "off",
  "max-depth": [
    "warn",
    {
      "max": 3
    }
  ],
  "linebreak-style": "off",
  "react/no-array-index-key": [
    "off"
  ],
  "max-len": [
    "off"
  ],
  "react/forbid-prop-types": [
    1,
    {
      "forbid": [
        "any"
      ]
    }
  ],
  "react/jsx-filename-extension": [
    1,
    {
      "extensions": [
        ".js",
        ".jsx"
      ]
    }
  ],
  "react/no-danger": [
    "off"
  ],
  "import/extensions": [
    "error",
    "always",
    {
      "js": "never",
      "jsx": "never"
    }
  ],
  "jsx-a11y/label-has-for": [
    0,
    {
      "components": [
        "Label"
      ],
      "allowChildren": true
    }
  ],
  "no-console": [
    "error",
    {
      "allow": [
        "warn",
        "error"
      ]
    }
  ],
  "comma-dangle": [
    "off",
    "always-multiline"
  ],
  "no-unused-vars": [
    "warn",
    {
      "args": "none",
      "caughtErrors": "all"
    }
  ],
  "no-shadow": [
    "off",
    {
      "allow": [
        "props"
      ]
    }
  ],
  "no-param-reassign": [
    "warn",
    {
      "props": false
    }
  ],
  "import/no-extraneous-dependencies": "off",
  "no-nested-ternary": "off",
  "radix": "off",
  "func-names": "off",
  "prefer-destructuring": "off",
  "newline-per-chained-call": [
    "error",
    {
      "ignoreChainWithDepth": 6
    }
  ],
  "no-plusplus": [
    2,
    {
      "allowForLoopAfterthoughts": true
    }
  ],
  "no-underscore-dangle": [
    "error",
    {
      "allow": [
        "_id",
        "_isMounted"
      ]
    }
  ]
  }
}
